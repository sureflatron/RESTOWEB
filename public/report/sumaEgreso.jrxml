<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="sumaEgreso" language="groovy" pageWidth="555" pageHeight="802" columnWidth="555" leftMargin="0" rightMargin="0" topMargin="0" bottomMargin="0" uuid="c4039cd9-ecdd-4e2f-8ad3-2361408902f7">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechaini" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechafin" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="idusuario" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="idsucursal" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT sum(Importe) as importe
FROM
(
SELECT
  SUM(egreso.importe) AS Importe
FROM
  sucursal,egreso
INNER JOIN
  tipoegreso ON tipoegreso.id = egreso.idTipoEgreso
INNER JOIN
  puntoventa ON puntoventa.id = egreso.idPuntoVenta
INNER JOIN
  empleado ON empleado.id = puntoventa.idEmpleado and empleado.id=$P{idusuario}
WHERE
  egreso.fecha BETWEEN $P{fechaini}  AND  $P{fechafin}  AND egreso.eliminado = 0
  and egreso.formapago = 1 and puntoventa.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}
 UNION all
 Select   sum(c.total) as Importe
from sucursal,compra c
inner JOIN puntoventa pv on   c.idPuntoVenta=pv.id
 inner JOIN empleado e on e.id=pv.idEmpleado and e.id=$P{idusuario}
 
where c.fecha BETWEEN  $P{fechaini}  AND $P{fechafin} and c.formaPago ='Efectivo'  and c.estado = 1
AND c.eliminado=0 and pv.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}



UNION ALL


SELECT

  SUM(cobroacuota.importe) AS importe
FROM
sucursal,
  empleado,
  puntoventa,
  compra,
  cobroacuota,
  cuentaacobrar
WHERE
  compra.idPuntoVenta = puntoventa.id AND puntoventa.idEmpleado = empleado.id  AND compra.eliminado = 0 AND compra.formaPago = 'Credito' AND empleado.id =$P{idusuario} and  cuentaacobrar.idCompra = compra.id AND cobroacuota.idCuentaaCobrar = cuentaacobrar.id AND cobroacuota.fecha BETWEEN $P{fechaini} AND $P{fechafin} and compra.estado = 2
 AND puntoventa.idSucursal = sucursal.id AND sucursal.id = $P{idsucursal}
GROUP BY
  cobroacuota.fecha,cobroacuota.hora


UNION ALL

SELECT SUM(importe) as Importe from (
SELECT Compra.id,
        compra.acuenta AS importe
    
        FROM sucursal,Compra 
        INNER JOIN puntoventa
        INNER JOIN empleado
        WHERE Compra.idpuntoventa=puntoventa.id 
                   AND puntoventa.idEmpleado=empleado.id
				   AND empleado.id=$P{idusuario}
                   AND Compra.eliminado=0
                   AND Compra.formaPago = 'Credito' and compra.fecha BETWEEN $P{fechaini} AND $P{fechafin}  and puntoventa.idSucursal = sucursal.id
                   AND sucursal.id = $P{idsucursal}
        ORDER BY id DESC)img


) gg]]>
	</queryString>
	<field name="importe" class="java.math.BigDecimal">
		<fieldDescription><![CDATA[]]></fieldDescription>
	</field>
	<background>
		<band splitType="Stretch"/>
	</background>
