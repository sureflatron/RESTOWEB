	<queryString>
		<![CDATA[select format(sum(Importe),2) as importeTotal
FROM
(
(SELECT -(sum(Importe)) as importe
FROM
(
SELECT
  SUM(egreso.importe) AS Importe
FROM
  sucursal,egreso
INNER JOIN
  tipoegreso ON tipoegreso.id = egreso.idTipoEgreso
INNER JOIN
  puntoventa ON puntoventa.id = egreso.idPuntoVenta
INNER JOIN
  empleado ON empleado.id = puntoventa.idEmpleado and empleado.id=$P{idusuario}
WHERE
  egreso.fecha BETWEEN $P{fechaini} AND $P{fechafin}  AND egreso.eliminado = 0 
  and egreso.formapago = 1  and puntoventa.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}
 UNION all



Select   sum(c.total) as Importe
from sucursal,compra c
inner JOIN puntoventa pv on   c.idPuntoVenta=pv.id
 inner JOIN empleado e on e.id=pv.idEmpleado and e.id=$P{idusuario}
 
where c.fecha BETWEEN  $P{fechaini}  AND $P{fechafin} and c.formaPago ='Efectivo' and c.estado = 1
AND c.eliminado=0  and pv.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}


UNION ALL


SELECT

  SUM(cobroacuota.importe) AS importe
FROM
sucursal,
  empleado,
  puntoventa,
  compra,
  cobroacuota,
  cuentaacobrar
WHERE
  compra.idPuntoVenta = puntoventa.id AND puntoventa.idEmpleado = empleado.id  AND compra.eliminado = 0 AND compra.formaPago = 'Credito' AND empleado.id =$P{idusuario} and  cuentaacobrar.idCompra = compra.id AND cobroacuota.idCuentaaCobrar = cuentaacobrar.id AND cobroacuota.fecha BETWEEN $P{fechaini} AND $P{fechafin} and compra.estado = 2
AND puntoventa.idSucursal = sucursal.id AND sucursal.id = $P{idsucursal}
GROUP BY
  cobroacuota.fecha,cobroacuota.hora



UNION ALL

SELECT SUM(importe) as Importe from (
SELECT Compra.id,
        compra.acuenta  AS importe
    
        FROM sucursal, Compra 
        INNER JOIN puntoventa
        INNER JOIN empleado
        WHERE Compra.idpuntoventa=puntoventa.id 
                   AND puntoventa.idEmpleado=empleado.id
				   AND empleado.id=$P{idusuario}
                   AND Compra.eliminado=0
                   AND Compra.formaPago = 'Credito' and compra.fecha BETWEEN $P{fechaini} AND $P{fechafin}  and puntoventa.idSucursal = sucursal.id
                   AND sucursal.id = $P{idsucursal}
        ORDER BY id DESC)img



) gg)


union all





SELECT sum(Importe) as importe
FROM
(
SELECT
   sum(ingreso.importe) AS Importe
FROM
  sucursal,ingreso
INNER JOIN
  tipoingreso ON tipoingreso.id = ingreso.idTipoIngreso
 INNER JOIN
  puntoventa ON puntoventa.id = ingreso.idPuntoVenta
INNER JOIN
  empleado ON empleado.id = puntoventa.idEmpleado and empleado.id=$P{idusuario}
WHERE
  ingreso.fecha BETWEEN $P{fechaini} AND $P{fechafin} AND ingreso.eliminado = 0
  and ingreso.formapago = 1 and puntoventa.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}
 UNION all



select Importe as Importe from (
SELECT

  (
    venta.total - venta.`importedescuento`
  ) as Importe,
venta.id
FROM
  venta
INNER JOIN
  puntoventa ON puntoventa.id = venta.idPuntoVenta
INNER JOIN
  empleado ON empleado.id = puntoventa.idEmpleado AND empleado.id = $P{idusuario}
INNER JOIN
  sucursal ON sucursal.id = puntoventa.idSucursal  and sucursal.id = $P{idsucursal}
INNER JOIN
  empresa ON empresa.id = sucursal.idEmpresa
INNER JOIN
  cliente ON cliente.`id` = venta.`idCliente`
WHERE
  venta.`eliminado` = 0 AND venta.estado = 1 AND venta.fecha BETWEEN $P{fechaini} AND $P{fechafin} AND venta.formaPago = "Efectivo"  GROUP BY venta.id
)asd

UNION ALL




SELECT
  SUM(COBROCUOTA.importe) AS importe
FROM
sucursal,
  empleado,
  puntoventa,
  venta,
  COBROCUOTA,
  CUENTACOBRAR
WHERE
  venta.idPuntoVenta = puntoventa.id AND puntoventa.idEmpleado = empleado.id AND empleado.id =$P{idusuario} AND venta.eliminado = 0 AND venta.formaPago = 'Credito' AND CUENTACOBRAR.idVenta = venta.id AND COBROCUOTA.IdCuentaCobrar = CUENTACOBRAR.id AND cobrocuota.fecha BETWEEN $P{fechaini} AND $P{fechafin} and venta.estado = 1  AND puntoventa.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}
GROUP BY
  cobrocuota.fecha,cobrocuota.hora




  
  UNION ALL  
    
SELECT SUM(importe) as Importe from (
SELECT venta.id,
        
        venta.acuenta AS importe

        FROM sucursal,venta 
        INNER JOIN puntoventa 
        INNER JOIN empleado
        WHERE venta.idPuntoVenta=puntoventa.id 
                   AND puntoventa.idEmpleado=empleado.id
				   AND empleado.id=$P{idusuario}
                   AND venta.eliminado=0
                   AND venta.formaPago = 'Credito' and venta.fecha BETWEEN $P{fechaini} AND $P{fechafin} AND puntoventa.idSucursal = sucursal.id and sucursal.id = $P{idsucursal}
        ORDER BY id DESC
    
    )img  
    
    
) gg



)gg]]>
